{
	"name": "taomu-toolkit",
	"version": "0.1.4",
	"private": false,
	"type": "module",
	"main": "./dist/main.cjs",
	"module": "./dist/main.js",
	"types": "./types/main.d.ts",
	"exports": {
		"types": "./types/main.d.ts",
		"import": "./dist/main.js",
		"require": "./dist/main.cjs"
	},
	"files": [
		"README.md",
		"types",
		"dist"
	],
	"scripts": {
		"pu": "npm run test && npm run build && npm publish",
		"build": "node ./scripts/rm-dist.js && tsc --declaration & vite build",
		"dev": "npm run test:ui",
		"test": "vitest run",
		"test:ui": "vitest --ui",
		"test:dev": "vitest",
		"build:docs": "typedoc"
	},
	"dependencies": {
		"lodash": "^4.17.21"
	},
	"devDependencies": {
		"@types/lodash": "^4.17.7",
		"@types/node": "^22.5.5",
		"@vitest/ui": "^2.1.1",
		"jsdom": "^25.0.0",
		"typedoc": "^0.26.7",
		"typescript": "^5.6.2",
		"vite": "^5.4.7",
		"vitest": "^2.1.1"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	}
}